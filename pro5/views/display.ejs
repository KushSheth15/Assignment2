<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Document</title>
</head>

<body>
  <table border="2" align="center" height="45%" width="60%">
    <% details.forEach(element=> { %>
      <tr>
        <td>
          <h2>
            <%= element.name %>
          </h2>
        </td>
        <td>
          <h2>
            <%= element.email %>
          </h2>
        </td>
        <td>
          <h2>
            <%= element.course %>
          </h2>
        </td>
        <td><img src="/uploads/<%= element.profile %> " alt="" /></td>
        <td><a href="/delete/<%= element.id %>">Delete</a></td>
      </tr>
      <% }); %>
  </table>
</body>

</html>